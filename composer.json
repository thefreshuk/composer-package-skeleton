{
    "name": "thefreshuk/skeleton",
    "description": "Skeleton composer project for Leadflo PHP packages.",
    "type": "library",
    "readme": "./README.md",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "brianium/paratest": "^6.2",
        "vimeo/psalm": "^4.6",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Dunne",
            "email": "james@thefreshuk.com"
        },
        {
            "name": "Gabriel Carpreau",
            "email": "gabriel@thefreshuk.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {},
    "autoload": {
        "psr-4": {
            "Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skeleton\\Tests\\": "tests/"
        }
    }
