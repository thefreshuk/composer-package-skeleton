{
    "name": "thefreshuk/skeleton",
    "description": "Skeleton composer project for Leadflo PHP packages.",
    "type": "library",
    "readme": "./README.md",
    "require-dev": {
        "ext-pcov": "*",
        "phpunit/phpunit": "^9.5",
        "brianium/paratest": "^6.2",
        "vimeo/psalm": "^4.6",
        "squizlabs/php_codesniffer": "^3.5",
        "infection/infection": "^0.21.0",
        "slevomat/coding-standard": "^6.4",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1"
    },
    "license": "MIT",
    "authors": [],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skeleton\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
